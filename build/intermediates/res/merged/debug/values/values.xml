        <item name="android:listViewStyle">@style/ThemeOneLisetViewStyle</item>
         <!-- 用户皮肤定义1 -->
        <item name="android:textViewStyle">@style/ThemeOneTextViewStyle</item>
        <item name="android:spinnerStyle">@style/ThemeOneSpinnerStyle</item>
