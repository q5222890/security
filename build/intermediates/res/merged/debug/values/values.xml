    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.







        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="ThemeOneButtonStyle" parent="@android:style/Widget.Button">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:textSize">@dimen/all_text_size</item>
        <!-- 设置背景绘制对象，可以是图片，这里是一个背景配置文件 -->
        <item name="android:background">@drawable/all_button_bg_color</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:height">@dimen/all_button_height</item>
        <item name="android:width">@dimen/all_button_width</item>
    </style>
    <style name="ThemeOneDialogStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:buttonStyle">@style/ThemeOneButtonStyle</item>
        <item name="android:editTextStyle">@style/ThemeOneEditBoxStyle</item>
        <item name="android:radioButtonStyle">@style/ThemeOneRadioButtomStyle</item>
    </style>
    <style name="ThemeOneEditBoxStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:textColorHint">@color/white_color</item>
        <item name="android:textSize">@dimen/all_text_size</item>
        <!-- 设置背景绘制对象，可以是图片，这里是一个背景配置文件 -->
        <item name="android:editable">true</item>
        <item name="android:height">@dimen/edit_box_hight</item>
        <item name="android:background">@drawable/edittext_custom_bg_color</item>
        <!-- 开启读入调用，没有这句设定的话不能输入文字 -->
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="ThemeOneEditSpinnerImageButtomStyle">
        <item name="android:background">@drawable/text_input_image_buttom_one</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="ThemeOneLisetViewStyle">
        <item name="android:background">@color/list_back_color</item>
        <item name="android:textColor">@color/white_color</item>
    </style>
    <style name="ThemeOneMainGridViewStyle">
        <item name="android:background">@color/list_back_color</item>
        <item name="android:textColor">@color/pale_gray_color</item>
        <item name="android:textSize">@dimen/all_text_size</item>
        <item name="android:shadowColor">@color/white_color</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">2</item>
    </style>
    <style name="ThemeOneMainTitleBackgroundStyle">
        <!-- <item name="android:background">@drawable/title_back</item> -->
    </style>
    <style name="ThemeOneRadioButtomStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:textColor">@color/white_color</item>
        <item name="android:drawableLeft">@drawable/checkbox_custom_bg_color</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/all_text_size</item>
        <item name="android:clickable">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <style name="ThemeOneSpinnerStyle">
        <item name="android:textColor">@color/pale_gray_color</item>
        <item name="android:textSize">@dimen/all_text_size</item>
        <item name="android:background">@drawable/spinner_custom_one</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:editable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="ThemeOneTextViewStyle">
        <item name="android:textColor">@color/white_color</item>
        <item name="android:gravity">right</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textSize">@dimen/all_text_size</item>
    </style>
    <style name="ThemeTwoDialogStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:buttonStyle">@style/ThemeOneButtonStyle</item>
        <item name="android:editTextStyle">@style/ThemeOneEditBoxStyle</item>
        <item name="android:radioButtonStyle">@style/ThemeOneRadioButtomStyle</item>
    </style>
    <style name="ac_query_btn_style">
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/ac_query_tb</item>
        <item name="android:drawableLeft">@null</item>
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">10dp</item>
    </style>
